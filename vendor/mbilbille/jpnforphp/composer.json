{
  "name": "jpnforphp/jpnforphp",
  "type": "library",
  "description": "A tiny PHP lib which provides nice functions & wrappers to interact with Japanese language.",
  "keywords": [
    "library",
    "japanese"
  ],
  "homepage": "http://mbilbille.github.com/jpnforphp/",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthieu Bilbille",
      "email": "matthieu.bilbille@gmail.com",
      "homepage": "https://github.com/mbilbille"
    }
  ],
  "support": {
    "source": "https://github.com/mbilbille/jpnforphp/tree/master",
    "issues": "https://github.com/mbilbille/jpnforphp/issues"
  },
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "symfony/yaml": ">=2.3"
  },
  "replace": {
    "jpnforphp/analyzer": "self.version",
    "jpnforphp/helper": "self.version",
    "jpnforphp/transliterator": "self.version",
    "jpnforphp/converter": "self.version",
    "jpnforphp/inflector": "self.version"
  },
  "autoload": {
    "psr-0": {
      "JpnForPhp": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9-dev"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
